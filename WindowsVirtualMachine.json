{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "newStorageAccountName": {
      "type": "string",
      "metadata": {
        "description": "The name of the new storage account created to store the VMs disks"
      }
    },
    "storageAccountType": {
      "type": "string",
      "defaultValue": "Standard_LRS",
      "allowedValues": [
        "Standard_LRS",
        "Standard_ZRS",
        "Standard_GRS",
        "Standard_RAGRS",
        "Premium_LRS"
      ],
      "metadata": {
        "description": "The type of storage account you want created"
      }
    },
      "DC01Name": {
        "type": "string",
        "minLength": 1
      },
      "DC01AdminUserName": {
        "type": "string",
        "minLength": 1
      },
      "DC01AdminPassword": {
        "type": "securestring"
      },
      "DC01WindowsOSVersion": {
        "type": "string",
        "defaultValue": "2012-R2-Datacenter",
        "allowedValues": [
          "2008-R2-SP1",
          "2012-Datacenter",
          "2012-R2-Datacenter",
          "Windows-Server-Technical-Preview",
          "2016-Nano-Server",
          "2016-Datacenter-with-Containers",
          "2016-Datacenter"
        ]
      },
      "Server01Name": {
        "type": "string",
        "minLength": 1
      },
      "Server01AdminUserName": {
        "type": "string",
        "minLength": 1
      },
      "Server01AdminPassword": {
        "type": "securestring"
      },
      "Server01WindowsOSVersion": {
        "type": "string",
        "defaultValue": "2012-R2-Datacenter",
        "allowedValues": [
          "2008-R2-SP1",
          "2012-Datacenter",
          "2012-R2-Datacenter",
          "Windows-Server-Technical-Preview",
          "2016-Nano-Server",
          "2016-Datacenter-with-Containers",
          "2016-Datacenter"
        ]
      },
      "DC02Name": {
        "type": "string",
        "minLength": 1
      },
      "DC02AdminUserName": {
        "type": "string",
        "minLength": 1
      },
      "DC02AdminPassword": {
        "type": "securestring"
      },
      "DC02WindowsOSVersion": {
        "type": "string",
        "defaultValue": "2012-R2-Datacenter",
        "allowedValues": [
          "2008-R2-SP1",
          "2012-Datacenter",
          "2012-R2-Datacenter",
          "Windows-Server-Technical-Preview",
          "2016-Nano-Server",
          "2016-Datacenter-with-Containers",
          "2016-Datacenter"
        ]
      },
      "PublicIP-DC01DnsName": {
        "type": "string",
        "minLength": 1
      },
      "PublicIP-Server01DnsName": {
        "type": "string",
        "minLength": 1
      },
      "PublicIP-DC02DnsName": {
        "type": "string",
        "minLength": 1
      },
      "PublicIP-FS01DnsName": {
        "type": "string",
        "minLength": 1
      },
      "PublicIP-FS02DnsName": {
        "type": "string",
        "minLength": 1
      },
      "domainName": {
        "type": "string",
        "metadata": {
          "description": "The FQDN of the AD Domain created "
        }
      },
      "assetLocation": {
        "type": "string",
        "metadata": {
          "description": "Where to find assets used in this template"
        }
      },
      "FS01Name": {
        "type": "string",
        "minLength": 1
      },
      "FS01AdminUserName": {
        "type": "string",
        "minLength": 1
      },
      "FS01AdminPassword": {
        "type": "securestring"
      },
      "FS01WindowsOSVersion": {
        "type": "string",
        "defaultValue": "2016-Nano-Server",
        "allowedValues": [
          "2008-R2-SP1",
          "2012-Datacenter",
          "2012-R2-Datacenter",
          "Windows-Server-Technical-Preview",
          "2016-Nano-Server",
          "2016-Datacenter-with-Containers",
          "2016-Datacenter"
        ]
      },
      "FS02Name": {
        "type": "string",
        "minLength": 1
      },
      "FS02AdminUserName": {
        "type": "string",
        "minLength": 1
      },
      "FS02AdminPassword": {
        "type": "securestring"
      },
      "FS02WindowsOSVersion": {
        "type": "string",
        "defaultValue": "2016-Nano-Server",
        "allowedValues": [
          "2008-R2-SP1",
          "2012-Datacenter",
          "2012-R2-Datacenter",
          "Windows-Server-Technical-Preview",
          "2016-Nano-Server",
          "2016-Datacenter-with-Containers",
          "2016-Datacenter"
        ]
      }

    },
    "variables": {
      "VNetName": "VNet1",
      "VNet1Prefix": "192.168.0.0/16",
      "VNet1Subnet1Name": "Production",
      "VNet1Subnet1Prefix": "192.168.95.0/24",
      "VNet1Subnet2Name": "Future",
      "VNet1Subnet2Prefix": "192.168.96.0/24",
      "ojradcliffestorageName": "[concat('ojrad', uniqueString(resourceGroup().id))]",
      "DC01ImagePublisher": "MicrosoftWindowsServer",
      "DC01ImageOffer": "WindowsServer",
      "DC01OSDiskName": "DC01OSDisk",
      "DC01VmSize": "Standard_D2_v2",
      "DC01VnetID": "[resourceId('Microsoft.Network/virtualNetworks', variables('VNetName'))]",
      "DC01SubnetRef": "[concat(variables('DC01VnetID'), '/subnets/', variables('VNet1Subnet1Name'))]",
      "DC01StorageAccountContainerName": "vhds",
      "DC01NicName": "[concat(parameters('DC01Name'), 'Nic0')]",
      "Server01ImagePublisher": "MicrosoftWindowsServer",
      "Server01ImageOffer": "WindowsServer",
      "Server01OSDiskName": "Server01OSDisk",
      "Server01VmSize": "Standard_D2_v2",
      "Server01VnetID": "[resourceId('Microsoft.Network/virtualNetworks', variables('VNetName'))]",
      "Server01SubnetRef": "[concat(variables('Server01VnetID'), '/subnets/', variables('VNet1Subnet1Name'))]",
      "Server01StorageAccountContainerName": "vhds",
      "Server01NicName": "[concat(parameters('Server01Name'), 'Nic0')]",
      "DC02ImagePublisher": "MicrosoftWindowsServer",
      "DC02ImageOffer": "WindowsServer",
      "DC02OSDiskName": "DC02OSDisk",
      "DC02VmSize": "Standard_D2_v2",
      "DC02VnetID": "[resourceId('Microsoft.Network/virtualNetworks', variables('VNetName'))]",
      "DC02SubnetRef": "[concat(variables('DC02VnetID'), '/subnets/', variables('VNet1Subnet1Name'))]",
      "DC02StorageAccountContainerName": "vhds",
      "DC02NicName": "[concat(parameters('DC02Name'), 'Nic0')]",
      "FS01ImagePublisher": "MicrosoftWindowsServer",
      "FS01ImageOffer": "WindowsServer",
      "FS01OSDiskName": "FS01OSDisk",
      "FS01VmSize": "Standard_D2_v2",
      "FS01VnetID": "[resourceId('Microsoft.Network/virtualNetworks', variables('VNetName'))]",
      "FS01SubnetRef": "[concat(variables('FS01VnetID'), '/subnets/', variables('VNet1Subnet1Name'))]",
      "FS01StorageAccountContainerName": "vhds",
      "FS01NicName": "[concat(parameters('FS01Name'), 'Nic0')]",
      "FS02ImagePublisher": "MicrosoftWindowsServer",
      "FS02ImageOffer": "WindowsServer",
      "FS02OSDiskName": "FS02OSDisk",
      "FS02VmSize": "Standard_D2_v2",
      "FS02VnetID": "[resourceId('Microsoft.Network/virtualNetworks', variables('VNetName'))]",
      "FS02SubnetRef": "[concat(variables('FS02VnetID'), '/subnets/', variables('VNet1Subnet1Name'))]",
      "FS02StorageAccountContainerName": "vhds",
      "FS02NicName": "[concat(parameters('FS02Name'), 'NetworkInterface')]",
      "PublicIP-DC01Name": "PublicIP-DC01",
      "PublicIP-Server01Name": "PublicIP-Server01",
      "PublicIP-DC02Name": "PublicIP-DC02",
      "PublicIP-FS01Name": "PublicIP-FS01",
      "PublicIP-FS02Name": "PublicIP-FS02",
      "PrivateIP-DC01": "192.168.95.10",
      "PrivateIP-Server01": "192.168.95.11",
      "PrivateIP-DC02": "192.168.95.12",
      "PrivateIP-FS01": "192.168.95.13",
      "PrivateIP-FS02": "192.168.95.14",
      "DSCURL": "[concat(parameters('assetLocation'),'DSCAssets.zip')]",
      "DC01DSCscript": "CreateFirstDC.ps1",
      "DC01DSCfunction": "CreateFirstDC",
      "DC02DSCscript": "CreateSecondDC.ps1",
      "DC02DSCfunction": "CreateSecondDC",
      "FS01DSCscript": "fileShareFS01DSC.ps1",
      "FS01DSCfunction": "fileShareFS01Config",
      "FS02DSCscript": "fileShareFS02DSC.ps1",
      "FS02DSCfunction": "fileShareFS02Config",
      "adPopulationData": "populationData.psd1"
    },
    "resources": [
      {
        "name": "[variables('VNetName')]",
        "type": "Microsoft.Network/virtualNetworks",
        "location": "[resourceGroup().location]",
        "apiVersion": "2015-06-15",
        "dependsOn": [],
        "tags": {
          "displayName": "VNet1"
        },
        "properties": {
          "addressSpace": {
            "addressPrefixes": [
              "[variables('VNet1Prefix')]"
            ]
          },
          "subnets": [
            {
              "name": "[variables('VNet1Subnet1Name')]",
              "properties": {
                "addressPrefix": "[variables('VNet1Subnet1Prefix')]"
              }
            },
            {
              "name": "[variables('VNet1Subnet2Name')]",
              "properties": {
                "addressPrefix": "[variables('VNet1Subnet2Prefix')]"
              }
            }
          ]
        }
      },
      {
        "name": "[variables('ojradcliffestorageName')]",
        "type": "Microsoft.Storage/storageAccounts",
        "location": "[resourceGroup().location]",
        "apiVersion": "2015-06-15",
        "dependsOn": [],
        "tags": {
          "displayName": "ojradcliffestorage"
        },
        "properties": {
          "accountType": "[parameters('ojradcliffestorageType')]"
        }
      },
      {
        "name": "[variables('DC01NicName')]",
        "type": "Microsoft.Network/networkInterfaces",
        "location": "[resourceGroup().location]",
        "apiVersion": "2015-06-15",
        "dependsOn": [
          "[resourceId('Microsoft.Network/virtualNetworks', variables('VNetName'))]",
          "[resourceId('Microsoft.Network/publicIPAddresses', variables('PublicIP-DC01Name'))]"
        ],
        "tags": {
          "displayName": "DC01Nic"
        },
        "properties": {
          "ipConfigurations": [
            {
              "name": "ipconfig1",
              "properties": {
                "privateIPAllocationMethod": "Static",
                "privateIPAddress": "[variables('PrivateIP-DC01')]",
                "subnet": {
                  "id": "[variables('DC01SubnetRef')]"
                },
                "publicIPAddress": {
                  "id": "[resourceId('Microsoft.Network/publicIPAddresses', variables('PublicIP-DC01Name'))]"
                }
              }
            }
          ]
        }
      },
      {
        "name": "[parameters('DC01Name')]",
        "type": "Microsoft.Compute/virtualMachines",
        "location": "[resourceGroup().location]",
        "apiVersion": "2015-06-15",
        "dependsOn": [
          "[resourceId('Microsoft.Storage/storageAccounts', variables('ojradcliffestorageName'))]",
          "[resourceId('Microsoft.Network/networkInterfaces', variables('DC01NicName'))]"
        ],
        "tags": {
          "displayName": "DC01"
        },
        "properties": {
          "hardwareProfile": {
            "vmSize": "[variables('DC01VmSize')]"
          },
          "osProfile": {
            "computerName": "[parameters('DC01Name')]",
            "adminUsername": "[parameters('DC01AdminUsername')]",
            "adminPassword": "[parameters('DC01AdminPassword')]",
            "windowsConfiguration": {
              "timeZone": "Central Standard Time"
            }
          },
          "storageProfile": {
            "imageReference": {
              "publisher": "[variables('DC01ImagePublisher')]",
              "offer": "[variables('DC01ImageOffer')]",
              "sku": "[parameters('DC01WindowsOSVersion')]",
              "version": "latest"
            },
            "osDisk": {
              "name": "DC01OSDisk",
              "vhd": {
                "uri": "[concat('https://', variables('ojradcliffestorageName'), '.blob.core.windows.net/', variables('DC01StorageAccountContainerName'), '/', variables('DC01OSDiskName'), '.vhd')]"
              },
              "caching": "ReadWrite",
              "createOption": "FromImage"
            }
          },
          "networkProfile": {
            "networkInterfaces": [
              {
                "id": "[resourceId('Microsoft.Network/networkInterfaces', variables('DC01NicName'))]"
              }
            ]
          }
        },
        "resources": [
          {
            "name": "CreateFirstDC",
            "type": "extensions",
            "location": "[resourceGroup().location]",
            "apiVersion": "2016-03-30",
            "dependsOn": [
              "[resourceId('Microsoft.Compute/virtualMachines', parameters('DC01Name'))]"
            ],
            "tags": {
              "displayName": "DC01DSC"
            },
            "properties": {
              "publisher": "Microsoft.Powershell",
              "type": "DSC",
              "typeHandlerVersion": "2.21",
              "autoUpgradeMinorVersion": true,
              "settings": {
                "wmfVersion": "latest",
                "configuration": {
                  "url": "[variables('DSCURL')]",
                  "script": "[variables('DC01DSCscript')]",
                  "function": "[variables('DC01DSCfunction')]"
                },
                "configurationArguments": {
                  "domainName": "[parameters('domainName')]",
                  "adminCreds": {
                    "userName": "[parameters('DC01AdminUserName')]",
                    "Password": "PrivateSettingsRef:AdminPassword"
                  }
                }
              },
              "protectedSettings": {
                "Items": {
                  "adminPassword": "[parameters('DC01AdminPassword')"
                }
              }
            }
          }
        ]
      },
      {
        "name": "[variables('Server01NicName')]",
        "type": "Microsoft.Network/networkInterfaces",
        "location": "[resourceGroup().location]",
        "apiVersion": "2015-06-15",
        "dependsOn": [
          "[resourceId('Microsoft.Network/virtualNetworks', variables('VNetName'))]",
          "[resourceId('Microsoft.Network/publicIPAddresses', variables('PublicIP-Server01Name'))]"
        ],
        "tags": {
          "displayName": "Server01Nic"
        },
        "properties": {
          "ipConfigurations": [
            {
              "name": "ipconfig1",
              "properties": {
                "privateIPAllocationMethod": "Static",
                "privateIPAddress": "[variables('PrivateIP-Server01')]",
                "subnet": {
                  "id": "[variables('Server01SubnetRef')]"
                },
                "publicIPAddress": {
                  "id": "[resourceId('Microsoft.Network/publicIPAddresses', variables('PublicIP-Server01Name'))]"
                }
              }
            }
          ]
        }
      },
      {
        "name": "[parameters('Server01Name')]",
        "type": "Microsoft.Compute/virtualMachines",
        "location": "[resourceGroup().location]",
        "apiVersion": "2015-06-15",
        "dependsOn": [
          "[resourceId('Microsoft.Storage/storageAccounts', variables('ojradcliffestorageName'))]",
          "[resourceId('Microsoft.Network/networkInterfaces', variables('Server01NicName'))]"
        ],
        "tags": {
          "displayName": "Server01"
        },
        "properties": {
          "hardwareProfile": {
            "vmSize": "[variables('Server01VmSize')]"
          },
          "osProfile": {
            "computerName": "[parameters('Server01Name')]",
            "adminUsername": "[parameters('Server01AdminUsername')]",
            "adminPassword": "[parameters('Server01AdminPassword')]",
            "windowsConfiguration": {
              "timeZone": "Central Standard Time"
            }
          },
          "storageProfile": {
            "imageReference": {
              "publisher": "[variables('Server01ImagePublisher')]",
              "offer": "[variables('Server01ImageOffer')]",
              "sku": "[parameters('Server01WindowsOSVersion')]",
              "version": "latest"
            },
            "osDisk": {
              "name": "Server01OSDisk",
              "vhd": {
                "uri": "[concat('https://', variables('ojradcliffestorageName'), '.blob.core.windows.net/', variables('Server01StorageAccountContainerName'), '/', variables('Server01OSDiskName'), '.vhd')]"
              },
              "caching": "ReadWrite",
              "createOption": "FromImage"
            }
          },
          "networkProfile": {
            "networkInterfaces": [
              {
                "id": "[resourceId('Microsoft.Network/networkInterfaces', variables('Server01NicName'))]"
              }
            ]
          }
        },
        "resources": [
          {
            "name": "Server1JoinDomain",
            "type": "extensions",
            "location": "[resourceGroup().location]",
            "apiVersion": "2016-03-30",
            "dependsOn": [
              "[resourceId('Microsoft.Compute/virtualMachines', parameters('Server01Name'))]",
              "[concat('Microsoft.Compute/virtualMachines/', parameters('DC01Name'),'/extensions/CreateFirstDC')]",
              "Microsoft.Resources/deployments/updatenicdns"
            ],
            "tags": {
              "displayName": "Server01JoinDomain"
            },
            "properties": {
              "publisher": "Microsoft.Compute",
              "type": "JsonADDomainExtension",
              "typeHandlerVersion": "1.3",
              "autoUpgradeMinorVersion": true,
              "settings": {
                "Name": "[parameters('domainName')]",
                "User": "[concat(parameters('domainName'), '\\', parameters('DC01AdminUserName'))]",
                "Restart": "true",
                "Options": "3"
              },
              "protectedSettings": {
                "password": "[parameters('DC01AdminPassword')]"
              }
            }
          }
        ]
      },
      {
        "name": "[variables('DC02NicName')]",
        "type": "Microsoft.Network/networkInterfaces",
        "location": "[resourceGroup().location]",
        "apiVersion": "2015-06-15",
        "dependsOn": [
          "[resourceId('Microsoft.Network/virtualNetworks', variables('VNetName'))]",
          "[resourceId('Microsoft.Network/publicIPAddresses', variables('PublicIP-DC02Name'))]"
        ],
        "tags": {
          "displayName": "DC02Nic"
        },
        "properties": {
          "ipConfigurations": [
            {
              "name": "ipconfig1",
              "properties": {
                "privateIPAllocationMethod": "Static",
                "privateIPAddress": "[variables('PrivateIP-DC02')]",
                "subnet": {
                  "id": "[variables('DC02SubnetRef')]"
                },
                "publicIPAddress": {
                  "id": "[resourceId('Microsoft.Network/publicIPAddresses', variables('PublicIP-DC02Name'))]"
                }
              }
            }
          ]
        }
      },
      {
        "name": "[parameters('DC02Name')]",
        "type": "Microsoft.Compute/virtualMachines",
        "location": "[resourceGroup().location]",
        "apiVersion": "2015-06-15",
        "dependsOn": [
          "[resourceId('Microsoft.Storage/storageAccounts', variables('ojradcliffestorageName'))]",
          "[resourceId('Microsoft.Network/networkInterfaces', variables('DC02NicName'))]"
        ],
        "tags": {
          "displayName": "DC02"
        },
        "properties": {
          "hardwareProfile": {
            "vmSize": "[variables('DC02VmSize')]"
          },
          "osProfile": {
            "computerName": "[parameters('DC02Name')]",
            "adminUsername": "[parameters('DC02AdminUsername')]",
            "adminPassword": "[parameters('DC02AdminPassword')]",
            "windowsConfiguration": {
              "timeZone": "Central Standard Time"
            }
          },
          "storageProfile": {
            "imageReference": {
              "publisher": "[variables('DC02ImagePublisher')]",
              "offer": "[variables('DC02ImageOffer')]",
              "sku": "[parameters('DC02WindowsOSVersion')]",
              "version": "latest"
            },
            "osDisk": {
              "name": "DC02OSDisk",
              "vhd": {
                "uri": "[concat('https://', variables('ojradcliffestorageName'), '.blob.core.windows.net/', variables('DC02StorageAccountContainerName'), '/', variables('DC02OSDiskName'), '.vhd')]"
              },
              "caching": "ReadWrite",
              "createOption": "FromImage"
            }
          },
          "networkProfile": {
            "networkInterfaces": [
              {
                "id": "[resourceId('Microsoft.Network/networkInterfaces', variables('DC02NicName'))]"
              }
            ]
          }
        },
        "resources": [
          {
            "name": "CreateSecondDC",
            "type": "extensions",
            "location": "[resourceGroup().location]",
            "apiVersion": "2016-03-30",
            "dependsOn": [
              "[resourceId('Microsoft.Compute/virtualMachines', parameters('DC02Name'))]",
              "[concat('Microsoft.Compute/virtualMachines/', parameters('Server01Name'),'/extensions/Server1JoinDomain')]"
            ],
            "tags": {
              "displayName": "DC02DSC"
            },
            "properties": {
              "publisher": "Microsoft.Powershell",
              "type": "DSC",
              "typeHandlerVersion": "2.21",
              "autoUpgradeMinorVersion": true,
              "settings": {
                "wmfVersion": "latest",
                "configuration": {
                  "url": "[variables('DSCURL')]",
                  "script": "[variables('DC02DSCscript')]",
                  "function": "[variables('DC02DSCfunction')]"
                },
                "configurationdata": {
                  "url": "[concat(parameters('assetLocation'), variables('adPopulationData'))]"
                },
                "configurationArguments": {
                  "domainName": "[parameters('domainName')]",
                  "adminCreds": {
                    "userName": "[parameters('DC01AdminUserName')]",
                    "password": "[parameters('DC01AdminPassword')]"
                  }
                }
              },
              "protectedSettings": {}
            }
          }
        ]
      },
      {
        "name": "[variables('FS01NicName')]",
        "type": "Microsoft.Network/networkInterfaces",
        "location": "[resourceGroup().location]",
        "apiVersion": "2015-06-15",
        "dependsOn": [
          "[resourceId('Microsoft.Network/virtualNetworks', variables('VNetName'))]"
        ],
        "tags": {
          "displayName": "FS01Nic"
        },
        "properties": {
          "ipConfigurations": [
            {
              "name": "ipconfig1",
              "properties": {
                "privateIPAllocationMethod": "Static",
                "privateIPAddress": "[variables('PrivateIP-FS01')]",
                "subnet": {
                  "id": "[variables('FS01SubnetRef')]"
                },
                "publicIPAddress": {
                  "id": "[resourceId('Microsoft.Network/publicIPAddresses', variables('PublicIP-FS01Name'))]"
                }
              }
            }
          ]
        }
      },
      {
        "name": "[parameters('FS01Name')]",
        "type": "Microsoft.Compute/virtualMachines",
        "location": "[resourceGroup().location]",
        "apiVersion": "2015-06-15",
        "dependsOn": [
          "[resourceId('Microsoft.Storage/storageAccounts', variables('ojradcliffestorageName'))]",
          "[resourceId('Microsoft.Network/networkInterfaces', variables('FS01NicName'))]"
        ],
        "tags": {
          "displayName": "FS01"
        },
        "properties": {
          "hardwareProfile": {
            "vmSize": "[variables('FS01VmSize')]"
          },
          "osProfile": {
            "computerName": "[parameters('FS01Name')]",
            "adminUsername": "[parameters('FS01AdminUsername')]",
            "adminPassword": "[parameters('FS01AdminPassword')]",
            "windowsConfiguration": {
              "timeZone": "Central Standard Time"
            }
          },
          "storageProfile": {
            "imageReference": {
              "publisher": "[variables('FS01ImagePublisher')]",
              "offer": "[variables('FS01ImageOffer')]",
              "sku": "[parameters('FS01WindowsOSVersion')]",
              "version": "latest"
            },
            "osDisk": {
              "name": "FS01OSDisk",
              "vhd": {
                "uri": "[concat('https://', variables('ojradcliffestorageName'), '.blob.core.windows.net/', variables('FS01StorageAccountContainerName'), '/', variables('FS01OSDiskName'), '.vhd')]"
              },
              "caching": "ReadWrite",
              "createOption": "FromImage"
            }
          },
          "networkProfile": {
            "networkInterfaces": [
              {
                "id": "[resourceId('Microsoft.Network/networkInterfaces', variables('FS01NicName'))]"
              }
            ]
          }
        },
        "resources": [
          {
            "name": "FS01DSC",
            "type": "extensions",
            "location": "[resourceGroup().location]",
            "apiVersion": "2016-03-30",
            "dependsOn": [
              "[resourceId('Microsoft.Compute/virtualMachines', parameters('FS01Name'))]",
              "[concat('Microsoft.Compute/virtualMachines/', parameters('Server01Name'),'/extensions/Server1JoinDomain')]",
              "[concat('Microsoft.Compute/virtualMachines/', parameters('DC02Name'),'/extensions/CreateSecondDC')]"
            ],
            "tags": {
              "displayName": "FS01DSC"
            },
            "properties": {
              "publisher": "Microsoft.Powershell",
              "type": "DSC",
              "typeHandlerVersion": "2.21",
              "autoUpgradeMinorVersion": true,
              "settings": {
                "wmfVersion": "latest",
                "configuration": {
                  "url": "[variables('DSCURL')]",
                  "script": "[variables('FS01DSCscript')]",
                  "function": "[variables('FS01DSCfunction')]"
                },
                "configurationdata": {
                  "url": "[concat(parameters('assetLocation'), variables('adPopulationData'))]"
                },
                "configurationArguments": {
                  "domainName": "[parameters('domainName')]",
                  "adminCreds": {
